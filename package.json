{
  "name": "cyclus",
  "description": "IOC and lifecycle for javascript object",
  "version": "1.2.3",
  "author": "hung-phan",
  "dependencies": {
    "ts-jest": "^20.0.7",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^20.0.4",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "engines": {
    "node": ">= 4.1.0",
    "npm": ">=3.0.0"
  },
  "files": [
    "dist",
    "src",
    "preprocessor.js",
    "README.md",
    "tsconfig.json"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testMatch": [
      "**/__tests__/*.spec.(ts|js)"
    ]
  },
  "jsnext:main": "dist/esnext/index.js",
  "license": "MIT",
  "keywords": [
    "dependency injection",
    "inversion of control",
    "lifecycle"
  ],
  "main": "dist/es6/index.js",
  "module": "dist/esnext/index.js",
  "scripts": {
    "build": "npm run clean && tsc --target es5 --outDir dist/es5 && tsc --target es6 --outDir dist/es6 && tsc --target esnext --outDir dist/esnext",
    "clean": "rimraf dist",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "typings": "dist/es6/index.d.ts"
}
