{
  "name": "cyclus",
  "description": "IOC and lifecycle for javascript object",
  "version": "1.1.2",
  "author": "hung-phan",
  "dependencies": {
    "lodash": "^4.17.4",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "invariant": "^2.2.2",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "ts-node": "^3.2.0",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0"
  },
  "engines": {
    "node": ">= 4.1.0",
    "npm": ">=3.0.0"
  },
  "files": [
    "dist",
    "src",
    "preprocessor.js",
    "README.md",
    "tsconfig.json"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.spec.(ts|js)"
    ]
  },
  "jsnext:main": "dist/esnext/index.js",
  "license": "MIT",
  "main": "dist/es6/index.js",
  "module": "dist/esnext/index.js",
  "scripts": {
    "build": "npm run clean && tsc --target es6 --outDir dist/es6 && tsc --target esnext --outDir dist/esnext",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "typings": "dist/es6/index.d.ts"
}
